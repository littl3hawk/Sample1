  "otherScripts": {
    "postinstall": "npm run typings install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "app": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "typings": "typings"
  },
  "scriptsPreCli": {
    "wp_start": "webpack-dev-server --inline --progress --port 8787",
    "wp_test": "karma start",
    "wp_build": "rimraf dist && webpack --config src/config/webpack.prod.js --progress --profile --bail",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:css": "node-sass  -r src  -o src  --output-style compressed  --linefeed lf   --source-map true",
    "build:css:standard": "node-sass --include-path scss src/sample.scss   src/sample.css",
    "build:css:notworking": "node-sass  -r src  -o src  --output-style compressed  --linefeed lf   --source-map true",
    "build:watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run build:watch:css\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint2": "tslint --init"
  },
  "dependenciesPreCli": {
    "systemjs": "0.19.40"
  },
  "devDependenciesPreCli": {
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "3.1.2",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.4.0"
  }
